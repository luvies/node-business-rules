{
  "name": "@luvies/business-rules",
  "version": "0.6.1",
  "description": "A JS-expression-based rules engine",
  "main": "dist/index.js",
  "repository": "https://github.com/luvies/node-business-rules",
  "author": "luvies",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "lint": "./node_modules/@luvies/config/scripts/lint.sh",
    "fix": "./node_modules/@luvies/config/scripts/lint.sh fix",
    "test": "jest --expand",
    "test:ci": "jest --ci --expand",
    "test:watch": "jest --watch",
    "test:coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "yarn build && yarn test",
    "postpublish": "git push && git push --tags",
    "clean": "rm -rf dist/* *.tsbuildinfo"
  },
  "dependencies": {
    "@luvies/evaluator": "^0.2.0",
    "coveralls": "^3.0.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@luvies/config": "^1.4.3",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "files": [
    "dist",
    "package.json",
    "yarn.lock",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "business",
    "rules",
    "jsep",
    "expression"
  ]
}
